{
  "name": "express-projects",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-css": "stylus src/css/index.styl -o public/css",
    "watch-css": "stylus src/css/index.styl -o public/css -w",
    "clean": "rm -rf public/css && mkdir -p public/css",
    "build": "npm run clean && npm run build-css",
    "watch": "npm run clean && npm run watch-css & nodemon app -e js,pug",
    "start": "node app"
  },
  "dependencies": {
    "express": "^4.14.1",
    "morgan": "^1.8.1",
    "nib": "^1.0.4",
    "pug": "^2.0.0-beta11",
    "stylus": "^0.49.1"
  }
}
